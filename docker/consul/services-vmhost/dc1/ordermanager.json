{
  "service": {
    "name": "ordermanager",
    "id": "ordermanager-dc1",
    "address": "__HOST_IP__",
    "port": 8081,
    "meta": {
      "datacenter": "dc1",
      "instanceRole": "primary"
    },
    "check": {
      "id": "ordermanager-health",
      "http": "http://__HOST_IP__:8081/actuator/health",
      "interval": "5s",
      "timeout": "1s",
      "failures_before_critical": 3,
      "success_before_passing": 3
    },
    "connect": {
      "sidecar_service": {
        "port": 21001,
        "proxy": {
          "upstreams": [
            {
              "destination_name": "refdata",
              "local_bind_address": "0.0.0.0",
              "local_bind_port": 18182
            }
          ]
        }
      }
    }
  }
}
